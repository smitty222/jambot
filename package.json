      "mocha"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "nodemon": "^3.1.9",
    "nyc": "^15.1.0",
    "standard": "^17.0.0"
  },
  "dependencies": {
    "@cometchat/chat-sdk-javascript": "^4.0.10",
    "@google/generative-ai": "^0.22.0",
    "bard-ai": "^2.0.3",
    "bard-api-node": "^2.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fast-json-patch": "^3.1.1",
    "node-fetch": "^3.2.10",
    "openai": "^4.90.0",
    "redis": "^4.6.7",
    "remove-markdown": "^0.5.0",
    "repeat": "^2.0.4",
    "socket.io-client": "^4.5.2",
    "string-similarity": "^4.0.4",
